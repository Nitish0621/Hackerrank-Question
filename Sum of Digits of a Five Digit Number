#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
	
    int n,sum=0,x,y;
    scanf("%d", &n);
    //Complete the code to calculate the sum of the five digits on n.
    x=n;
    while(x!=0)
    {
        y=x%10;
        sum=sum+y;
        x=x/10;
    }
    printf("%d",sum);
    return 0;
}
